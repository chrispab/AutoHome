Thing mqtt:topic:amplifier  "MQTT Thing: ir amplifier" (mqtt:broker:MyMQTTBroker) @ "Conservatory" {
    Channels:
    Type switch : standby "ir amplifier standby" [ stateTopic="irbridge/stat/amplifier/standby", commandTopic="irbridge/cmnd/amplifier/standby", on="on", off="off" ]
    Type switch : power_on "ir amplifier power on" [ stateTopic="irbridge/stat/amplifier/power_on", commandTopic="irbridge/cmnd/amplifier/power_on", on="on", off="off" ]
    Type switch : power_off "ir amplifier power off" [ stateTopic="irbridge/stat/amplifier/power_off", commandTopic="irbridge/cmnd/amplifier/power_off", on="on", off="off" ]
    Type switch : video1 "ir amplifier video1" [ stateTopic="irbridge/stat/amplifier/video1", commandTopic="irbridge/cmnd/amplifier/video1", on="on", off="off" ]
    Type switch : aux "ir amplifier aux" [ stateTopic="irbridge/stat/amplifier/aux", commandTopic="irbridge/cmnd/amplifier/aux", on="on", off="off" ]
    Type switch : tuner "ir amplifier tuner" [ stateTopic="irbridge/stat/amplifier/tuner", commandTopic="irbridge/cmnd/amplifier/tuner", on="on", off="off" ]
    Type switch : tape1 "ir amplifier source tape1" [ stateTopic="irbridge/stat/amplifier/tape1_mon", commandTopic="irbridge/cmnd/amplifier/tape1_mon", on="on", off="off" ]
    Type switch : mute "ir amplifier mute" [ stateTopic="irbridge/stat/amplifier/mute", commandTopic="irbridge/cmnd/amplifier/mute", on="on", off="off" ]
    Type switch : volumeup "ir amplifier volume up" [ stateTopic="irbridge/stat/amplifier/volume_up", commandTopic="irbridge/cmnd/amplifier/volume_up", on="on", off="off" ]
    Type switch : volumedown "ir amplifier volume down" [ stateTopic="irbridge/stat/amplifier/volume_down", commandTopic="irbridge/cmnd/amplifier/volume_down", on="on", off="off" ]
    Type number : code "ir amplifier control code" [ stateTopic="irbridge/stat/amplifier/code", commandTopic="irbridge/cmnd/amplifier/code" ]
    Type switch : reachable "ir amplifier reachable" [ stateTopic="irbridge/LWT", on="Online", off="Offline" ]
    Type number : rssi "ir amplifier control rssi" [ stateTopic="irbridge/stat/rssi" ]
    Type string : ir_bridge_version "ir bridge software version" [ stateTopic="irbridge/stat/version"]



}
