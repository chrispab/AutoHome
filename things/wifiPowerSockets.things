Thing mqtt:topic:WiFiSocket1  "MQTT Thing: WiFi Power Socket 1" (mqtt:broker:MyMQTTBroker) @ "YetToBePlaced"
{
    Channels:
        Type switch : powerSwitch  [ stateTopic="wiFiSocket1/stat/POWER" , commandTopic="wiFiSocket1/cmnd/POWER", on="ON", off="OFF" ]
        Type string : Reachable    [ stateTopic="wiFiSocket1/tele/LWT"  ]
        Type number : rssi         [ stateTopic="wiFiSocket1/tele/STATE",  transformationPattern="JSONPATH:$.Wifi.RSSI" ]
}

