// https://community.openhab.org/t/a-couple-of-simple-oh-repeater-examples/116818

Group gChristmasWifiPowerSockets

Group:Switch:OR(ON,OFF) gAllChristmasLights "Christmas Lights" { ga="Switch" }
Group:Switch:OR(ON,OFF) gFrontRoomChristmasLights "Front Room Christmas Lights" { ga="Switch" }
Group:Switch:OR(ON,OFF) gHallChristmasLights "Hall Christmas Lights" { ga="Switch" }
Group:Switch:OR(ON,OFF) gDiningRoomChristmasLights "Dining Room Christmas Lights" { ga="Switch" }

Switch xmas_lights_bg_wifisocket_3_1_power "xmas_lights_bg_wifisocket_3_1_power" (gMQTT, gBGWiFiPowerSockets, gAllChristmasLights, gFrontRoomChristmasLights)  { channel = "mqtt:topic:bg_wifisocket_3:powerSwitch_1" }
Switch xmas_lights_bg_wifisocket_3_2_power "xmas_lights_bg_wifisocket_3_2_power" (gMQTT, gBGWiFiPowerSockets, gAllChristmasLights, gFrontRoomChristmasLights)  { channel = "mqtt:topic:bg_wifisocket_3:powerSwitch_2" }

