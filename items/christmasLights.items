Switch xmas_lights_bg_wifisocket_5_2_power "xmas_lights_bg_wifisocket_5_2_power" (gMQTT, gBGWiFiPowerSockets, gAllChristmasLights, gFrontRoomChristmasLights)  { channel = "mqtt:topic:bg_wifisocket_5:powerSwitch_2" }

